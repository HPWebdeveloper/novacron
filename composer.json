{
  "name": "studio/novacron",
  "description": "Manage schedule the nova way",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "scripts": {
    "test": "phpunit && npm run test"
  },
  "require": {
    "php": ">=7.1.0",
    "owenmelbz/nova-radio-field": "^0.0.5",
    "studio/laravel-totem": "@dev"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "laravel/nova": "~1.0",
    "orchestra/testbench": "^3.6",
    "phpunit/phpunit": "7.1"
  },
  "autoload": {
    "psr-4": {
      "Studio\\Novacron\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Studio\\Novacron\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Studio\\Novacron\\ToolServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": {
    "totem": {
      "type": "path",
      "url": "../laravel-totem"
    },
    "remote": {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  }
}
